{
  "name": "tinnitus-relief-pro",
  "version": "1.0.0",
  "description": "Notched Sound Therapy & Frequency Matching for Tinnitus Relief",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "npx http-server . -p 8080 -o",
    "ios": "npx cap sync ios && npx cap open ios",
    "android": "npx cap sync android && npx cap open android",
    "build": "echo 'No build step required for static HTML app'",
    "sync": "npx cap sync",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "tinnitus",
    "sound-therapy",
    "notched-audio",
    "health",
    "wellness"
  ],
  "author": "Paulabac, LLC",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "happy-dom": "^13.3.0",
    "http-server": "^14.1.1",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6"
  },
  "capacitor": {
    "appId": "com.paulabac.tinnitusreliefpro",
    "appName": "Tinnitus Relief Pro",
    "webDir": ".",
    "plugins": {
      "SplashScreen": {
        "launchShowDuration": 2000
      }
    }
  }
}
